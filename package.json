{
  "name": "blitz-func",
  "version": "1.8.0",
  "description": "nodejs+koa+typescript template made for tecent cloudbase function",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --config nodemon.json",
    "build": "ttsc -P tsconfig.json",
    "deploy": "zx ./scripts/deploy_min.mjs",
    "deploy:full": "zx ./scripts/deploy.mjs",
    "deploy:db": "tsc ./utils/db.ts --outDir ./utils && zx ./scripts/deploy_db.mjs && rm ./utils/db.js",
    "deploy:db:prod": "tsc ./utils/db.ts --outDir ./utils && zx ./scripts/deploy_db.mjs prod && rm ./utils/db.js"
  },
  "author": "jay4q.com",
  "license": "ISC",
  "dependencies": {
    "@cloudbase/node-sdk": "latest",
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.0.0",
    "dotenv": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "md5": "^2.3.0",
    "serverless-http": "^2.7.0",
    "serverless-mysql": "^1.5.4"
  },
  "devDependencies": {
    "@cloudbase/manager-node": "latest",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.2",
    "@types/koa__router": "^8.0.7",
    "@types/node": "^16.3.2",
    "@vercel/ncc": "^0.33.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.12",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "ttypescript": "^1.5.12",
    "typescript": "4.2.3",
    "typescript-transform-paths": "2.2.3",
    "zx": "^4.2.0"
  }
}
