{
  "envId": "{{env.TCB_ENVID}}",
  "functionRoot": "./functions",
  "functions": [
    {
      "name": "{{env.TCB_FUNC_NAME}}",
      "timeout": 10,
      "runtime": "Nodejs12.16",
      "installDependency": true,
      "handler": "index.main",
      "memorySize": 128,
      "envVariables": {
        "TZ": "Asia/Shanghai"
      },
      "vpc": {
        "vpcId": "{{env.TCB_VPC_ID}}",
        "subnetId": "{{env.TCB_VPC_SUBNET_ID}}"
      }
    }
  ]
}